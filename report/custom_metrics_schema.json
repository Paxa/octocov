{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "key": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "metrics": {
            "type": "array",
            "items":
            {
                "type": "object",
                "properties": {
                    "key": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "value": {
                        "type": "number"
                    },
                    "unit": {
                        "type": "string"
                    }
                },
                "required": [
                    "key",
                    "value"
                ]
            }
        }
    },
    "required": [
        "key",
        "metrics"
    ]
}
